#include<stdio.h>
void main(){
	int i,n,a[1000],count=0;
	printf("Enter the size:");
	scanf("%d",&n);
	printf("Enter the elements:");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
for(i=0;i<n;i++)
{
	if(a[i]==2)
	{
		continue;
	}
	else if(a[i]%2==0){
	
		count++;
	}
}
if (count>2){
}
printf("number of composite numbers are:%d",count);
}
	
